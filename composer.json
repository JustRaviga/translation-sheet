{
    "name": "JustRaviga/translation-sheet",
    "description": "Laravel Translation via Google Spreadsheet",
    "homepage": "https://github.com/JustRaviga/translation-sheet",
    "license": "MIT",
    "authors": [
        {
            "name": "Nassif Bourguig",
            "email": "nbourguig@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "google/apiclient": "^2.1",
        "illuminate/console": "^8.0||^9.0||^10.0",
        "illuminate/filesystem": "^8.0||^9.0||^10.0",
        "illuminate/support": "^8.0||^9.0||^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11",
        "orchestra/testbench": "^6.0",
        "codedungeon/phpunit-result-printer": "^0.26.2"
    },
    "autoload": {
        "psr-4": {
            "Nikaia\\TranslationSheet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nikaia\\TranslationSheet\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Nikaia\\TranslationSheet\\TranslationSheetServiceProvider"
            ]
        },
        "branch-alias": {
            "^8.0||^9.0||^10.0": "1.3-dev"
        }
    },
    "prefer-stable": true
}
